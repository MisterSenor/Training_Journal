
<%@workouts.each do |workout|%>
  <strong>Workout creator:</strong> <%=workout.user.username%></br>
  <strong>Workout description:</strong> <%=workout.description%></br>
  <strong>Workout intensity:</strong> <%=workout.intensity%></br>
  <%= link_to "Edit workout", edit_workout_path(workout)%></br>
  <%= button_to "Delete workout", workout_path(workout), method: :delete, data: { confirm: "Are you sure that you want to delete this workout?"}%></br></br>
  <%end%>
